{
    "manifest_version": 3,
    "name": "Swwager Copy1",
    "description": "Copy path ",
    "version": "1.0",
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "permissions": [
        "storage",
        "activeTab",
        "bookmarks",
        "scripting",
        "cookies",
        "tabs"
    ],
    "host_permissions": [
        "https://*/*"
    ],
    "action": {
        "default_popup": "./popup.html"
    },
    "content_scripts": [
        {
            "js": [
                "content.js"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "cross_origin_embedder_policy": {
        "value": "require-corp"
    },
    "cross_origin_opener_policy": {
        "value": "same-origin"
    },
    "background": {
        "service_worker": "./background.js"
    }
}